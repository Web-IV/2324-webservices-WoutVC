{
  "name": "webservices-budget",
  "version": "0.0.1",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npx env-cmd nodemon",
    "lint": "eslint .",
    "test": "npx env-cmd -f .env.test jest",
    "test:coverage": "npx env-cmd -f .env.test jest --coverage"
  },
  "nodemonConfig": {
    "signal": "SIGTERM",
    "ignore": [
      "node_modules/*",
      ".vscode/*"
    ],
    "delay": "80",
    "watch": [
      "config",
      "src"
    ],
    "ext": "js,json",
    "legacyWatch": true,
    "exec": "node --inspect=0.0.0.0:9001 --trace-warnings src/index.js"
  },
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "bcrypt": "^5.1.0",
    "config": "^3.3.8",
    "env-cmd": "^10.1.0",
    "jest": "^29.6.2",
    "joi": "^17.9.2",
    "knex": "^2.3.0",
    "koa": "^2.14.1",
    "koa-bodyparser": "^4.3.0",
    "mysql2": "^2.3.3",
    "sequelize": "^6.32.1",
    "supertest": "^6.3.3",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "eslint": "^8.46.0",
    "nodemon": "^2.0.20"
  }
}
